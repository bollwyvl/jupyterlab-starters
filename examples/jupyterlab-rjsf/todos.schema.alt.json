{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/deathbeds/jupyterlab-starters/master/packages/jupyterlab-rjsf/examples/todo.schema.json",
  "title": "# TODO THESE THINGS",
  "type": "object",
  "description": "A List of Things.",
  "properties": {
    "todos": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "text": {
            "title": "This is a Thing",
            "description": "It's a _very_ important thing",
            "type": "string",
            "pattern": "^[A-Z].*"
          },
          "done": {
            "type": "boolean"
          },
          "importance": {
            "title": "",
            "type": "number"
          }
        }
      }
    }
  }
}
