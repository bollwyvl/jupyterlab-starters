{
  "StarterManager": {
    "starters": {
      "whitepaper-single": {
        "type": "copy",
        "label": "Whitepaper (One)",
        "description": "A reusable notebook for proposing research",
        "src": "examples/whitepaper-single.ipynb",
        "commands": [
          {
            "id": "application:set-mode",
            "args": {
              "mode": "single-document"
            }
          },
          {
            "id": "filebrowser:open-path",
            "args": {
              "path": "whitepaper-single.ipynb"
            }
          }
        ]
      },
      "whitepaper-multiple": {
        "type": "copy",
        "label": "Whitepaper (Many)",
        "description": "Some reusable notebooks for proposing research",
        "src": "examples/whitepaper-multiple",
        "commands": [
          {
            "id": "filebrowser:open-path",
            "args": {
              "path": "whitepaper-multiple/00 Introduction.ipynb"
            }
          }
        ]
      }
    }
  }
}
